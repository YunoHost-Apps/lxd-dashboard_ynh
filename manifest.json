{
    "name": "LXD Dashboard",
    "id": "lxd-dashboard",
    "packaging_format": 1,
    "description": {
        "en": "Makes it easy for you to take control of your LXD based infrastructure"
    },
    "version": "3.7.0~ynh1",
    "url": "https://lxdware.com",
    "upstream": {
        "license": "AGPL-3.0-or-later",
        "website": "https://lxdware.com",
        "code": "https://github.com/lxdware/lxd-dashboard"
    },
    "license": "AGPL-3.0-or-later",
    "maintainer": {
        "name": "yalh76"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.3-fpm",
        "mysql"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/lxd-dashboard",
                "default": "/lxd-dashboard"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "password",
                "type": "password"
            }
        ]
    }
}
